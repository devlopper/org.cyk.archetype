<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="global-pom"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <requiredProperties>
  	<requiredProperty key="groupId"><defaultValue>none</defaultValue></requiredProperty>
  	<requiredProperty key="artifactId"><defaultValue>pom</defaultValue></requiredProperty>
  	
  	<requiredProperty key="version"><defaultValue>0.0.1</defaultValue></requiredProperty>
  	
  	<requiredProperty key="systemId"/>
  	<requiredProperty key="rootVersion"><defaultValue>0.0.1</defaultValue> </requiredProperty>
  	<requiredProperty key="package"><defaultValue>org.cyk.system.${systemId}</defaultValue></requiredProperty>
  	
  </requiredProperties>

	<fileSets>
		<fileSet filtered="true">
			<directory></directory>
			<includes>
				<include>pom.xml</include>
			</includes>
		</fileSet>
		<!-- model -->
		
		<fileSet filtered="true">
			<directory>model</directory>
			<includes>
				<include>pom.xml</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" packaged="true">
			<directory>model/src/main/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
		</fileSet>
		<fileSet filtered="true">
			<directory>model/src/main/resources</directory>
			<includes>
				<include>**/*.*</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" packaged="true">
			<directory>model/src/test/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
		</fileSet>
		<fileSet filtered="true">
